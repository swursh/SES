{
    "manifest_version": 3,
    "name": "url Script Viewer",
    "version": "1.0",
    "description": "View scripts of the current page",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  